#Keyword Arguments
def welcome(str):
    print("Welcome to python,", str)
    return()
welcome(str="Raj")
  
#Default Arguments
def info (name,age=50):
    print ("Name: ", name )
    print ("Age:", age)
    return ()

info(age=25,name="Raj")
info(name="Kumar")

# Variable Length Arguments
def info(user,*users):
    print("User of Python:")
    print(user)
    for var in users:
        print("Users of Python:")
        print (var)
        return()
        
